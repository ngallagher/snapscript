function time(name, func){
   var s = System.currentTimeMillis();
   func();
   var f = System.currentTimeMillis();
   println(name +": "+(f-s));
}
const ITERATIONS = 10000000;

time("if{}",->{
   var x=0;
   for(var i in 0..100000000){
      if(i >0){
         x++;
      }
   }
});
