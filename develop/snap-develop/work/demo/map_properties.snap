class T {
   var a;
   var b;
   
   new(m){
      this.a = m.a;
      this.b = m.b;
   }
   
   toString(){
      return "${a} ${b}";
   }
}

var t = new T({a:11, b:"str"});


println(t);

var m = {a:11, b:"str"};

println(m);

m.a = 33;
m.c = "foo";

println(m);

m.a = null;

println(m);

function max(a, b) {
   return a > b ? a : b;
}

function max(a, b, c) { // function overloading
   return a > b ? max(a, c) : max(b, c);
}

println(max(2,33,4));