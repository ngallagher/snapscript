try {
   throw "this is some text 1 - catch String";
}catch(e: String){
   System.err.println(e);
}
try {
   try {
      throw "this is some text 2 - catch Integer";
   }catch(e: Integer){
      System.err.println("problem!!!! "+e);
   }
}catch(e){
   System.err.println("caught in last block '"+e + "'");
   
   try {
      throw new IllegalStateException(e);
   }catch(x: Exception) {
      x.printStackTrace();
   }
}
