var start = new Date().getTime();
calculate();

function calculate() {
      var pi = 4;
      var plus = false;
      for (var i = 3; i < 100000000; i += 2)
      {
          if (plus)
          {
              pi += 4.0 / i;
          }
          else
          {
              pi -= 4.0 / i;
          }
          plus = !plus;
       }
      System.err.println(pi);
}
var stop =new Date().getTime();
System.err.println(stop -start);
